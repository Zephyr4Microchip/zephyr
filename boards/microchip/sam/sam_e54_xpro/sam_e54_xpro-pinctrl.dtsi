#include <dt-bindings/pinctrl/sam/sam_d5x_e5x/same54/atsame54p20a-pinctrl.h>

&pinctrl {
	sercom2_uart_default: sercom2_uart_default {
		group1 {
			pinmux = <PB25D_SERCOM2_PAD0>,
					 <PB24D_SERCOM2_PAD1>;
		};
	};
	sercom6_spi_default: sercom6_spi_default {
		group1 {
			pinmux = <PC4C_SERCOM6_PAD0>,
			<PC7C_SERCOM6_PAD3>,
			<PC5C_SERCOM6_PAD1>,
			<PC6C_SERCOM6_PAD2>;
		};
	};
};
